CREATE\040TABLE\040tickets\040(
\040\040\040\040id\040INT\040AUTO_INCREMENT\040PRIMARY\040KEY,
\040\040\040\040title\040VARCHAR(255)\040NOT\040NULL,
\040\040\040\040description\040TEXT\040NOT\040NULL,
\040\040\040\040type\040ENUM('bug',\040'improvement',\040'request')\040NOT\040NULL,
\040\040\040\040priority\040ENUM('low',\040'medium',\040'high',\040'critical')\040NOT\040NULL,
\040\040\040\040status\040ENUM('new',\040'in_progress',\040'pending',\040'resolved',\040'closed')\040NOT\040NULL,
\040\040\040\040user_id\040INT\040NOT\040NULL,
\040\040\040\040admin_id\040INT\040DEFAULT\040NULL,
\040\040\040\040created_at\040TIMESTAMP\040DEFAULT\040CURRENT_TIMESTAMP,
\040\040\040\040updated_at\040TIMESTAMP\040DEFAULT\040CURRENT_TIMESTAMP\040ON\040UPDATE\040CURRENT_TIMESTAMP,
\040\040\040\040resolved_at\040TIMESTAMP\040NULL\040DEFAULT\040NULL
);
